javascript:(function(){
  var css = ".form-group:has(#btnOk){display:none!important;}";
  var s = document.getElementById("hideFormGroupCSS");
  if(!s){
      s=document.createElement("style");
      s.id="hideFormGroupCSS";
      s.appendChild(document.createTextNode(css));
      document.head.appendChild(s);
  }
})();
